{
    "name": "@lina/types",
    "version": "1.0.0",
    "description": "TypeScript type definitions for LINA",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist"
    },
    "dependencies": {},
    "devDependencies": {
        "typescript": "^5.5.4"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./api": {
            "types": "./dist/api.d.ts",
            "default": "./dist/api.js"
        },
        "./database": {
            "types": "./dist/database.d.ts",
            "default": "./dist/database.js"
        },
        "./analysis": {
            "types": "./dist/analysis.d.ts",
            "default": "./dist/analysis.js"
        }
    }
}
